<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.httprpc.demo" >
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="org.httprpc.demo.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="org.httprpc.demo.AddNoteActivity"
            android:label="@string/add_note">
        </activity>

        <activity
            android:name="org.httprpc.demo.NoteDetailActivity"
            android:label="@string/note_detail">
        </activity>
    </application>
</manifest>
